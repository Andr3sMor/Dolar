{
    "dev": {
        "app_function": "app.ejecutar_lambda", 
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "dollar-aws",
        "runtime": "python3.10",
        "s3_bucket": "dolar-raw-awsmm",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "Labrole",
        "events": [
            {
                "function": "app.ejecutar_lambda", 
                "expression": "cron(22 15 * * ? *)"
            }
        ]
    },
    "s3_to_mysql": {
        "app_function": "s3_to_mysql.g", 
        "aws_region": "us-east-1",
        "memory_size": 512,
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "zappa-s3-to-mysql",
        "runtime": "python3.10",
        "s3_bucket": "dolar-raw-awsmm",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "Labrole",
        "environment_variables": {
            "DB_HOST": "${DB_HOST}",
            "DB_USER": "${DB_USER}",
            "DB_PASS": "${DB_PASS}",
            "DB_NAME": "${DB_NAME}"
        },
        "events": [
            {
                "function": "s3_to_mysql.g", 
                "event_source": {
                    "arn": "arn:aws:s3:::dolar-raw-awsmm",
                    "events": ["s3:ObjectCreated:*"],
                    "key_filters": [{
                        "type": "suffix",
                        "value": ".json"
                    }]
                }
            }
        ]
    }
}
